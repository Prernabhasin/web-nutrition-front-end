{
    "manifest_version": 2,
    "name": "Web Nutrition",
    "version": "1.0.0",
    "description": "NewsCheck",
    "icons": {
        "16": "images/icon16.png"
    },
    "browser_action": {
        "default_icon": "images/Icon.png",
        "default_popup": "popup.html"
    },
    "background" : {
        "scripts": ["js/libs/jquery-3.3.1.min.js", "js/background.js"],
        "persistent": false
    },
    "permissions": [
        "background",
        "tabs"
    ]
}