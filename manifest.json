{
    "manifest_version": 2,
    "name": "Web Nutrition",
    "version": "1.0.0",
    "description": "NewsCheck",
    "icons": {
        "16": "images/icon16.png"
    },
    "browser_action": {
        "default_icon": "images/icon16.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ],
    "content_scripts": [{
        "js": [
            "js/libs/bootstrap.min.js",
            "js/libs/jquery-3.3.1.min.js",
            "js/libs/jquery.flip.min.js",
            "popup.js"
        ]
    }]
}